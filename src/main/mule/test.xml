		<logger level="INFO" doc:name="Logger" doc:id="14252ed3-d457-4c3e-b52b-9022a83bcd2e" message="JMSType='${activemq.JMSType.CancelledEvent}' OR JMSType='${activemq.JMSType.RescheduledEvent}' OR JMSType='${activemq.JMSType.DelayedEvent}'"/>
		<ee:transform doc:name="Transform Message" doc:id="0e77a038-d635-42c9-a2c1-cb40339cde25" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
	test: p('activemq.queue-consumer.flight-status-event-topic')
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
